<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CadastraPsicologoController</servlet-name>
        <servlet-class>br.edu.fescfafic.meem.control.CadastraPsicologoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginPsicologoController</servlet-name>
        <servlet-class>br.edu.fescfafic.meem.control.LoginPsicologoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarPacienteController</servlet-name>
        <servlet-class>br.edu.fescfafic.meem.control.CadastrarPacienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastraPacienteController</servlet-name>
        <servlet-class>br.edu.fescfafic.meem.control.CadastraPacienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarPacientesController</servlet-name>
        <servlet-class>br.edu.fescfafic.meem.control.ListarPacientesController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastraPsicologoController</servlet-name>
        <url-pattern>/CadastraPsicologoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPsicologoController</servlet-name>
        <url-pattern>/LoginPsicologoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarPacienteController</servlet-name>
        <url-pattern>/CadastrarPacienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastraPacienteController</servlet-name>
        <url-pattern>/CadastraPacienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarPacientesController</servlet-name>
        <url-pattern>/ListarPacientesController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
